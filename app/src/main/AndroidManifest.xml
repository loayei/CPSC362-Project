<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hikerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity android:name=".RegistrationActivity"/>
        <activity android:name=".ContactActivity"/>
        <activity android:name=".CreateContactActivity"/>
        <activity android:name=".ContactListActivity"/>
        <activity android:name=".EditContactActivity"/>
        <activity android:name=".GetContactsActivity"/>
        <activity android:theme="@android:style/Theme.Dialog" android:name=".DeleteContactActivity">

        </activity>
    </application>

</manifest>